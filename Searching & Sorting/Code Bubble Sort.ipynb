    "from sys import stdin\n",
    "\n",
    "def bubbleSort(arr, n) :\n",
    "    for i in range(n - 1) :\n",
    "        for j in range(n - i - 1) :\n",
    "            if arr[j] > arr[j+1] :\n",
    "                temp = arr[j] \n",
    "                arr[j] = arr[j + 1]\n",
    "                arr[j + 1] = temp "
